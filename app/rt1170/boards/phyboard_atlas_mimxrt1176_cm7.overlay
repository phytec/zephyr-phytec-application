/ {
	ramdisk0 {
		compatible = "zephyr,ram-disk";
		disk-name = "RAM";
		sector-size = <512>;
		sector-count = <192>;
	};
};

/delete-node/ &zephyr_udc0;

zephyr_udc0: &usb2 {
	status = "okay";
};

&zephyr_mipi_dsi {
	/* Raise the DSI clock frequency */
	phy-clock = <792000000>;
};
